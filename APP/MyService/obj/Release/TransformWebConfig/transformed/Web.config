<?xml version="1.0"?>
<configuration>

  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.serviceModel>
    <client>
      <endpoint address="net.msmq://localhost/private/ProdTracker/Service2.svc"
        binding="netMsmqBinding" bindingConfiguration="NetMsmqBinding_IService2"
        contract="ServiceRef.IService2" name="NetMsmqBinding_IService2" />
    </client>
    <services>
      <service name="MyService.Service1">
        <endpoint address="net.msmq://localhost/private/MyService/MyService.svc" binding="netMsmqBinding" contract="MyService.IService1" bindingConfiguration="test1">
          
        </endpoint>
      </service>
    </services>
    <bindings>
      <netMsmqBinding>
        <binding name="test1" exactlyOnce="false">
          <security mode="None" />
        </binding>
        <binding name="NetMsmqBinding_IService2" exactlyOnce="false">
          <security mode="None" />
        </binding>
      </netMsmqBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To avoid disclosing metadata information, set the values below to false before deployment -->
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>
          <!-- To receive exception details in faults for debugging purposes, set the value below to true.  Set to false before deployment to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <protocolMapping>
        <add binding="netMsmqBinding" scheme="http" />
    </protocolMapping>    
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
    <directoryBrowse enabled="true"/>
  </system.webServer>

</configuration>
